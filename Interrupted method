 class A extends Thread{
	
	public void run(){
		
		try{
			String s = Thread.currentThread().getName();
			for(int i=0;i<3;i++){
				System.out.println(s);
				Thread.sleep(1000);
			}
		}catch(Exception e){
			System.out.println("program Interrupted");
		}
	}	
}




public class Main {
	public static void main(String[] args)  throws Exception{
		
		A a = new A();
		
		a.start();
		a.interrupt();
		
		
	}
}
